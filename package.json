{
  "name": "gradle-versions-plugin-action",
  "version": "1.0.0",
  "private": true,
  "description": "Github Action for using https://github.com/ben-manes/gradle-versions-plugin results in github action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WtfJoke/gradle-versions-plugin-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "WtfJoke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WtfJoke/gradle-versions-plugin-action/issues"
  },
  "homepage": "https://github.com/WtfJoke/gradle-versions-plugin-action#readme",
  "dependencies": {
    "@actions/core": "^1.2.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.11.1",
    "@typescript-eslint/parser": "^2.25.0",
    "@zeit/ncc": "^0.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-github": "^3.4.1",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.2.3",
    "jest-circus": "^25.2.3",
    "js-yaml": "^3.13.1",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  }
}
